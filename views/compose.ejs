<%- include("partials/header"); -%>
<h1>Compose</h1>
<br>
<form class="" action="/compose" method="post">
  <div class="mb-3">
    <label class="form-label">Text</label>
    <input name="postTitle" type="text" class="form-control" id="formGroupExampleInput">
    <br>
    <label class="form-label">Post</label>
    <textarea name="postBody" type="text" class="form-control" rows="6" id="validationTextarea"></textarea>
  </div>
  <br>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

